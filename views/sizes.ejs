      <p>[Insert Canada size chart here]</p>
    </div>

    <div id="au-chart" class="size-chart">
      <h2>Australia Size Chart</h2>
      <p>[Insert Australia size chart here]</p>
    </div>


    <!-- T-shirts Row -->
    <div class="clothing-row">
      <div class="row-title">Tops</div>
      <div class="clothing-row-items" id="tshirts-row">
        <!-- Dynamic content will be inserted here -->
      </div>
    </div>

    <!-- Pants Row -->
    <div class="clothing-row">
      <div class="row-title">Bottoms</div>
      <div class="clothing-row-items" id="pants-row">
        <!-- Dynamic content will be inserted here -->
      </div>
    </div>

    <!-- Jackets Row -->
    <div class="clothing-row">
      <div class="row-title">Dresses</div>
      <div class="clothing-row-items" id="jackets-row">
        <!-- Dynamic content will be inserted here -->
      </div>
    </div>
  </div>

  <script>

function showCountryChart() {
      // Hide all charts first
      var selectedCountry = document.getElementById('country-selector').value;

      // Hide all charts first
      var charts = document.querySelectorAll('.size-chart');
      charts.forEach(chart => {
        chart.style.display = 'none';
      });

      // Show the selected country's chart if a valid country is selected
      if (selectedCountry) {
        var chartToShow = document.getElementById(selectedCountry + '-chart');
        if (chartToShow) {
          chartToShow.style.display = 'block';
        }
      }
    
  }

